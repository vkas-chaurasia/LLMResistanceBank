        {
            "block_label": "doc_title",
            "block_content": "# Non-Typhoidal Salmonella in poultry meat and diarrhoeic patients: prevalence, antibiogram, virulotyping, molecular detection and sequencing of class I integrons in multidrug resistant strains",
            "block_bbox": [
                106,
                235,
                1074,
                496
            ],
            "block_id": 5,
            "block_order": 3,
